<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qca.pagespeed.mapper.PageSpeedRequestHistoryMapper">
    <insert id="insertPageSpeedRequestHistory">
        INSERT INTO page_speed_request_history (url, reg_user, reg_date, mod_user, mod_date)
        VALUES (
            #{url},
            'SYSTEM',
            NOW(),
            'SYSTEM',
            NOW()
       )
    </insert>
</mapper>